{
    "manifest_version": 2,
    
    "name": "waitlist data page",
    "description": "This extension shows the size of a waitlist",
    "version": "1.0",
    "permissions": ["storage"],
    
    "content_scripts": [
        {
            "matches": ["https://utdirect.utexas.edu/registrar/waitlist/wl_see_my_waitlists.WBX/*"],
            "js": ["content.js"]
        }
    ]
    
    
}
